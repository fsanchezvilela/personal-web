{
  "bail": true,
  "verbose": true,
  "preset": "ts-jest",
  "globals": {
    "ts-jest": {
      "diagnostics": false
    }
  },
  "collectCoverage": false,
  "coverageDirectory": "./coverage/",
  "coverageReporters": ["lcov", "text"],
  "coveragePathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/dist/",
    "<rootDir>/public/",
    "<rootDir>/typings/",
    "<rootDir>/test/",
    "<rootDir>/e2eTests/"
  ],
  "collectCoverageFrom": [
    "src//*.(j|t)s",
    "!src//(actions|index|reducer|constants|constant|actionTypes|config|selectors|types).(j|t)s",
    "!/tests/",
    "!src//redux/reducers/*.(j|t)s"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 70,
      "lines": 80,
      "statements": 70
    }
  },
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/dist/",
    "<rootDir>/public/",
    "<rootDir>/typings/",
    "<rootDir>/e2eTests/",
    "<rootDir>/test/",
    "<rootDir>/coverage/"
  ],
  "setupFiles": ["./test/setup.js"],
  "testMatch": ["<rootDir>//tests/*/?((?!components).)+(spec|test).[jt]s?(x)"],
  "rootDir": "./",
  "modulePaths": ["./"],
  "transform": {
    "^.+\\.ts?$": "ts-jest",
    "^.+\\.js?$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src(.*)$": "<rootDir>/src$1",
    "^.+\\.(css|less|scss|sass)$": "identity-obj-proxy",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/globalMocks/fileMock.js",
    "array-move": "<rootDir>/test/globalMocks/arrayMoveMock.js"
  }
}